{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "atayahmet/laravel-nestable": "^0.8.7",
        "barryvdh/laravel-ide-helper": "^2.4",
        "bugsnag/bugsnag-laravel": "^2.0",
        "chumper/zipper": "^1.0",
        "facebook/graph-sdk": "~5.0",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "maatwebsite/excel": "^3.1",
        "mgp25/instagram-php": "^5.0",
        "plank/laravel-mediable": "^2.7",
        "pusher/pusher-http-laravel": "^4.2",
        "pusher/pusher-php-server": "v3.0.1",
        "spatie/laravel-activitylog": "^2.8",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.13",
        "twilio/sdk": "^5.25",
        "wa72/htmlpagedom": "^1.3"
    },
    "require-dev": {
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
