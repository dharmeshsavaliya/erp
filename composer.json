{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "atayahmet/laravel-nestable": "^0.8.7",
        "barryvdh/laravel-ide-helper": "^2.4",
        "bugsnag/bugsnag-laravel": "^2.0",
        "chumper/zipper": "^1.0",
        "doctrine/dbal": "^2.9",
        "dompdf/dompdf": "^0.8.3",
        "facebook/graph-sdk": "~5.0",
        "facebook/php-business-sdk": "^3.2",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.0",
        "guzzlehttp/guzzle": "^6.3",
        "intervention/image": "^2.4",
        "kosinix/grafika": "^2.0",
        "ksubileau/color-thief-php": "^1.4",
        "laravel/framework": "5.6.*",
        "laravel/passport": "^6.0",
        "laravel/tinker": "^1.0",
        "laravelcollective/html": "^5.6",
        "league/color-extractor": "^0.3.2",
        "league/csv": "^9.2",
        "maatwebsite/excel": "^3.1",
        "macsidigital/zoom-laravel": "^1.0",
        "mgp25/instagram-php": "^5.0",
        "nunomaduro/laravel-console-dusk": "^1.0",
        "ourcodeworld/name-that-color": "dev-master",
        "php-ai/php-ml": "^0.8.0",
        "phpoffice/phpspreadsheet": "^1.6",
        "plank/laravel-mediable": "^2.7",
        "pragmarx/tracker": "^3.3",
        "pusher/pusher-http-laravel": "^4.2",
        "pusher/pusher-php-server": "v3.0.1",
        "seo2websites/erp-excel-importer": "dev-master",
        "seo2websites/erp-pinterest": "dev-master",
        "seo2websites/google-vision": "dev-master",
        "seo2websites/laravel-zoom": "dev-master",
        "seo2websites/price-comparison-scraper": "dev-master",
        "skovmand/mailchimp-laravel": "^1.0",
        "spatie/laravel-activitylog": "^2.8",
        "spatie/laravel-analytics": "3.6.0",
        "spatie/laravel-image-optimizer": "1.3",
        "spatie/laravel-medialibrary": "^7.0.0",
        "spatie/laravel-permission": "^2.13",
        "techsemicolon/hubstaff": "^0.0.1",
        "thujohn/twitter": "^2.2",
        "twilio/sdk": "^5.25",
        "wa72/htmlpagedom": "^1.3",
        "webklex/laravel-imap": "^1.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^2.0",
        "seo2websites/erp-excel-importer": "dev-master",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@bitbucket.org:seo2websites/erp-pinterest"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:seo2websites/erp-excel-importer"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:seo2websites/google-vision"
        },
        {
            "type": "vcs",
            "url": "git@bitbucket.org:seo2websites/price-comparison-scrapper"
        },
        {
            "type": "vcs", 
            "url": "git@bitbucket.org:seo2websites/laravel-zoom"
        }
    ],
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
