{
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watch": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hot": "mix watch --hot",
        "prod": "npm run production",
        "production": "mix --production",
        "knowledge-base-build": "webpack",
        "knowledge-base-production": "SET NODE_ENV=production webpack && rm -f ./public/dist/*styles.js",
        "knowledge-base-build-profile": "SET NODE_ENV=production webpack --profile --json > webpack-stats.json && rm -f ./public/dist/*styles.js",
        "knowledge-base-dev": "npm-run-all --parallel watch livereload",
        "knowledge-base-watch": "webpack --watch",
        "knowledge-base-livereload": "livereload ./public/dist/",
        "knowledge-base-permissions": "chown -R $USER:$USER bootstrap/cache storage public/uploads",
        "format": "prettier --write ."
    },
    "devDependencies": {
        "axios": "^0.18",
        "bootstrap": "^4.3.1",
        "css-loader": "^2.1.1",
