        "livereload": "^0.8.0",
        "lodash": "^4.17.4",
        "mini-css-extract-plugin": "^0.7.0",
        "node-sass": "^8.0.0",
        "npm-run-all": "^4.1.5",
        "popper.js": "^1.14.7",
        "postcss": "^8.2.8",
        "sass-loader": "^7.1.0",
        "style-loader": "^0.23.1",
        "vue": "^2.5.7",
        "webpack": "^4.32.2",
        "webpack-cli": "^3.3.2"
    },
